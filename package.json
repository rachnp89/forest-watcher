{
  "name": "ForestWatcher",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "react": "15.4.1",
    "react-native": "0.38.0",
    "react-native-config": "^0.2.1",
    "react-native-i18n": "^0.1.1",
