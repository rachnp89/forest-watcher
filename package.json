            "routes": "./app/routes",
            "locales": "./app/locales",
            "config": "./app/config",
            "helpers": "./app/helpers",
            "components": "./app/components",
            "containers": "./app/containers",
            "redux-modules": "./app/redux-modules",
            "assets": "./app/assets"
          }
        }
      ]
    ]
  }
